<head>
	<meta http-equiv="content-type" content="text/html; charset=utf-8">
	<meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport">
	
	<!-- title -->
	<% 
		var title = page.title; 
		if ( title == '' ) {
			var title = page.slug.split("/")[page.slug.split("/").length-1]
		}else {
			var title = page.title; 
		};
	%>
	<title>
	<% if (title){ %>
		<%= title %> | 
	<% } %> 
	<%= config.title %>
	</title>
	
	<!-- keywords,description -->
	<% if (page.keywords){ %>
		<meta name="keywords" content="<%= page.keywords %>,<%= config.keywords %>" />
	<% } else if (config.keywords){ %>
		<meta name="keywords" content="<%= config.keywords %>" />
	<% } %> <% if (page.description){ %>
		<meta name="description" content="<%= page.description %>" />
	<% } else if (config.description){ %>
		<meta name="description" content="<%= config.description %>" />
	<% } %>

	<!-- favicon -->
	<% if (theme.favicon){ %>
	<%- favicon_tag(theme.favicon) %>
	<% } %>
  
	<%- css('css/main.css') %>
	<%- css('css/jquery.fancybox.min.css') %>
	<%- css('https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css') %>
	<%- css('lib/highlight/darcula.css') %>
	<%- css('css/clipboard-use.css') %>
	<%- css('css/APlayer.min.css') %>

	<%- js('lib/highlight/highlight.min.js') %>
	<%- js('https://readmore.openwrite.cn/js/readmore.js') %>
	<%- js('lib/jquery-3.4.1.min.js') %>
	<%- js('lib/jquery.pjax.js') %>
	<%- js('js/APlayer.min.js') %>

	<%- js('js/main.js') %>
	<%- js('js/jquery.fancybox.min.js') %>
	<% if (theme.valine){ %>
		<%- js('lib/valine/av-3.0.4-min.js') %>
		<%- js('lib/valine/Valine-1.3.10-min.js') %>
	<% } %>
	<% if (theme.busuanzi){ %>
		<script async src="//busuanzi.ibruce.info/busuanzi/2.3/busuanzi.pure.mini.js"></script>
	<% } %>
	<%- js('js/clipboard.min.js') %>
</head>